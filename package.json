{
	"name": "sveltekit-dashboard",
	"version": "0.0.1",
	"scripts": {
		"start": "node build/index.js",
		"dev": "vite dev",
		"build": "npm run smui-theme-light && npm run smui-theme-dark && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@playwright/test": "^1.21.0",
		"@smui-extra/accordion": "^6.0.0-beta.16",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/card": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-material-ui": "^6.0.0-beta.16",
		"svelte-preprocess": "^4.10.1",
		"typescript": "~4.6.2",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@mdi/js": "^6.6.96",
		"@smui/textfield": "^6.0.0-beta.16",
		"@sveltejs/adapter-node": "^1.0.0-next.73",
		"@types/dotenv": "^8.2.0",
		"@types/pg-promise": "^5.4.3",
		"dotenv": "^16.0.0",
		"pg-promise": "^10.11.1",
		"tslib": "^2.3.1"
	}
}
